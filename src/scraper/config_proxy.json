{
  "proxy_settings": {
    "rotation_strategy": "health_based",
    "health_check_interval": 300,
    "retry_failed_proxies": true,
    "max_retries_per_proxy": 3,
    "request_delay_range": [1, 3],
    "user_agent_rotation": true,
    "enable_tor": false
  },
  "proxies": [
    {
      "host": "proxy1.example.com",
      "port": 8080,
      "proxy_type": "http",
      "username": null,
      "password": null,
      "concurrent_limit": 5,
      "location": "US",
      "provider": "ProxyProvider1"
    },
    {
      "host": "proxy2.example.com", 
      "port": 8080,
      "proxy_type": "http",
      "username": "user123",
      "password": "pass123",
      "concurrent_limit": 3,
      "location": "EU",
      "provider": "ProxyProvider2"
    },
    {
      "host": "socks5.example.com",
      "port": 1080,
      "proxy_type": "socks5",
      "username": "sockuser",
      "password": "sockpass",
      "concurrent_limit": 2,
      "location": "UK",
      "provider": "SOCKSProvider"
    }
  ],
  "captcha_settings": {
    "enabled": true,
    "api_key": "YOUR_2CAPTCHA_API_KEY",
    "timeout": 120,
    "polling_interval": 5,
    "retry_count": 3
  },
  "tor_settings": {
    "enabled": false,
    "socks_port": 9050,
    "control_port": 9051,
    "control_password": "your_tor_password",
    "identity_rotation_interval": 600,
    "request_timeout": 30
  },
  "user_agent_settings": {
    "rotation_enabled": true,
    "rotation_strategy": "per_request",
    "browsers": ["chrome", "firefox"],
    "include_mobile": false,
    "custom_headers": {
      "Accept-Language": "en-US,en;q=0.9",
      "Accept-Encoding": "gzip, deflate, br",
      "DNT": "1",
      "Connection": "keep-alive",
      "Upgrade-Insecure-Requests": "1"
    }
  }
}
